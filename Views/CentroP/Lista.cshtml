@model List<Centro>

<h1>Lista de Gimnasios Pokemon Registrados</h1>

@if(Model.Count == 0){
    <div class="alert alert-warning text-center">
         Aun no hay centros pokemon registrados.
    </div>
} else{
    <ol>
        @foreach(var p in Model)
        {
            <li class="d-flex">
                <div>
                    @p.nombreCiudad                
                </div>
                <div>
                    @p.nombreLider
                </div>
                
            </li>
        }
    </ol>
}
